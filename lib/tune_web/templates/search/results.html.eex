<%= case {@q, @results} do %>
  <% {nil, _results} -> %>
    <div class="full-screen-notice">
      <p>Try and search for a song you love</p>
    </div>
  <% {_query, []} -> %>
    <div class="full-screen-notice">
      <p>No results</p>
    </div>
  <% {_query, results} -> %>
    <div class="results">
      <%= render_many results, TuneWeb.SearchView, "result.html", as: :item, socket: @socket %>
    </div>
<% end %>
