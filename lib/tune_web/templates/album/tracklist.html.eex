<ul class="tracks">
  <%= for track <- @tracks do %>
    <li>
      <span class="track-number"><%= track.track_number %></span>
      <span class="name"><%= track.name %></span>
      <span class="duration"><%= TuneWeb.PlayerView.format_duration(track.duration_ms) %></span>
    </li>
  <% end %>
</ul>
